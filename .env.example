# Copy this file to .env and fill in real values.
# .env is gitignored and should never be committed.

# Required once the model-backed node is wired in.
PERSISTENCE_AGENT_OPENAI_API_KEY=

# Optional model override for local runs.
OPENAI_MODEL=gpt-4o-mini

# Local Postgres defaults (Docker Compose).
DATABASE_URL=postgresql://app:app@127.0.0.1:5442/app
PGHOST=127.0.0.1
PGPORT=5442
PGDATABASE=app
PGUSER=app
PGPASSWORD=app

# Optional LangSmith tracing.
LANGSMITH_API_KEY=
LANGSMITH_TRACING=false
LANGSMITH_PROJECT=langgraph-persistence-skeleton
